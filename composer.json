{
    "name": "abk/query-bundle",
    "description": "Generic query component that can be used to generates specific queries via custom adapter.",
    "require": {
        "php": ">=5.5.9",
        "symfony/config": "2.6.*",
        "symfony/http-kernel": "2.6.*",
        "symfony/dependency-injection": "2.6.*",
        "doctrine/inflector": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "sebastian/phpcpd": "2.0.1",
        "phploc/phploc": "2.0.6",
        "pdepend/pdepend": "1.1.3",
        "squizlabs/php_codesniffer": "2.2.0",
        "phpmd/phpmd": "1.5.1",
        "covex-nn/phpcb": "1.0.4.1",
        "theseer/phpdox": "0.7.0",
        "phpdocumentor/phpdocumentor": "2.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.3-dev"
        }
    },
    "autoload": {
        "psr-0": { "ABK\\QueryBundle": "" }
    },
    "target-dir": "ABK/QueryBundle"
}
